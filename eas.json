{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_OPTIMIZE_IMAGES": "1"
        }
      },
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_OPTIMIZE_IMAGES": "1"
        }
      }
    },
    "production-apk": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "EXPO_OPTIMIZE_IMAGES": "1"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
